    <string name="aerr_application_permission_connectivity">„<xliff:g id="err_ap_name">%1$s</xliff:g>“ hat keinen Zugriff auf <xliff:g id="permission_name">%2$s</xliff:g>, du kannst <xliff:g id="permission_name2">%2$s</xliff:g> in „Einstellungen“ erlauben</string>
