package {
    default_applicable_licenses: ["vendor_transsion_license"],
}

// ==================== Native Demo 可执行文件 ====================

cc_binary {
    name: "perfengine_demo",
    system_ext_specific: true,

    srcs: ["perfengine_demo.cpp"],

    shared_libs: [
        "liblog",
        "libutils",
        "libcutils",
        "libbinder_ndk",
        "libandroid-base",
        "libperfengine-client",
        "vendor.transsion.hardware.perfengine-V1-ndk",
    ],

    header_libs: [
        "libperfengine-client-headers",
        "libandroid-base",
    ],

    cflags: ["-Wall", "-Wextra", "-Wno-unused-parameter"],
    cppflags: ["-std=c++17"],
}
