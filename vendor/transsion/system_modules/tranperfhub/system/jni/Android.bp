package {
    default_applicable_licenses: ["vendor_transsion_license"],
}

cc_library_shared {
    name: "libtranperfhub-jni",
    system_ext_specific: true,

    srcs: [
        "com_transsion_perfhub_TranPerfHub.cpp",
        "onload.cpp",
    ],

    header_libs: [
        "jni_headers",
        "libutils_headers",
        "liblog_headers",
    ],

    shared_libs: [
        "liblog",
        "libutils",
        "libcutils",
        "libnativehelper",
        "libandroid_runtime",
        "libbinder_ndk",
        "vendor.transsion.hardware.perfhub-ndk",
    ],

    static_libs: [
        "libtranperfhub_flags_c",
    ],

    cflags: [
        "-Wall",
        "-Werror",
        "-Wno-unused-parameter",
    ],
    cppflags: ["-std=c++17"],
}
