java_library {
    name: "mediatek-services",
    system_ext_specific: true,
    installable: true,
    srcs: [
        "core/java/com/mediatek/server/**/*.java",
        "core/java/com/mediatek/server/anr/EventLogTags.logtags",
    ],
    static_libs: [
        "services.mtksearchengineservice",
        "services.hdmilocalservice",
        "services.powerhalmgrservice",
        "duraspeednative",
        "com_mediatek_amplus",
        "services.vow",
        "jxnetworkpolicyservice"
    ],
    libs: [
        "service-jobscheduler",
        "services.core",
        "mediatek-framework",
        "mediatek-common",
        "android.hardware.power-V1.0-java",
        "vendor.mediatek.hardware.power-V2.0-java",
        "mediatek-framework",
    ],
    defaults: ["mediatek_services_defaults"],
    vintf_fragments: ["mbrainlocalservice/mtk-mbrainj-default.xml"],
}

bootstrap_go_package {
    name: "soong-services-mediatek",
    pkgPath: "android/soong/services/mediatek",
    deps: [
        "soong-android",
        "soong-java",
    ],
    srcs: [
        "soong.go",
    ],
    pluginFor: ["soong_build"],
}

mediatek_services_defaults {
    name: "mediatek_services_defaults",
}
