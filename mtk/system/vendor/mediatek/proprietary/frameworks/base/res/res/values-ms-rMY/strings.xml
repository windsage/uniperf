    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_bindTerminal">Apl dengan kebenaran ini mampu mengikat System\'s OMAPI terminal. SmartcardService hanya perlu menggunakannya.</string>
