    <string name="aerr_application_permission_connectivity">\"<xliff:g id="err_ap_name">%1$s</xliff:g>\" tidak boleh <xliff:g id="permission_name">%2$s</xliff:g>, anda boleh <xliff:g id="permission_name2">%2$s</xliff:g> dalam \"Tetapan\"</string>
